{
    "packages": {
        "newfold-labs/wp-module-framework": [
            {
                "name": "newfold-labs/wp-module-framework",
                "version": "dev-main",
                "version_normalized": "dev-main",
                "source": {
                    "type": "git",
                    "url": "git@github.com:newfold-labs/wp-module-framework.git",
                    "reference": "4110c54d69c43b537094eca5547c924251dbfbfa"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://api.github.com/repos/newfold-labs/wp-module-framework/zipball/4110c54d69c43b537094eca5547c924251dbfbfa",
                    "reference": "4110c54d69c43b537094eca5547c924251dbfbfa",
                    "shasum": ""
                },
                "require-dev": {
                    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
                    "newfold-labs/wp-php-standards": "^1.2.4",
                    "wp-cli/i18n-command": "^2.6.3"
                },
                "time": "2025-02-11T10:20:50+00:00",
                "default-branch": true,
                "type": "wordpress-plugin",
                "archive": {
                    "exclude": [
                        "./assets/js-src",
                        "./assets/scss",
                        "./bin",
                        "node_modules",
                        "./resources",
                        ".*",
                        ".github",
                        "*.md",
                        "./package.json",
                        "./package-lock.json",
                        "./webpack.config.js"
                    ]
                },
                "scripts": {
                    "i18n-pot-audit": [
                        "wp --allow-root i18n make-pot . --exclude=\".git,.idea,.vscode,bin,languages,node_modules,vendor,tests,dist\" --slug=wp-module-framework"
                    ],
                    "i18n-pot": [
                        "@i18n-pot-audit --skip-audit"
                    ],
                    "i18n-po": [
                        "wp --allow-root i18n update-po languages/wp-module-framework.pot"
                    ],
                    "i18n-mo": [
                        "wp --allow-root i18n make-mo languages"
                    ],
                    "i18n-php": [
                        "wp --allow-root i18n make-php languages"
                    ],
                    "php-fix": [
                        "phpcbf --standard=phpcs.xml ."
                    ],
                    "php-lint": [
                        "phpcs --standard=phpcs.xml -s ."
                    ]
                },
                "license": [
                    "GPL-2.0-or-later"
                ],
                "support": {
                    "source": "https://github.com/newfold-labs/wp-module-framework/tree/0.1.3",
                    "issues": "https://github.com/newfold-labs/wp-module-framework/issues"
                }
            }
        ]
    }
}
